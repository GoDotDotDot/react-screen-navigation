{
  "plugins": [
      "react"
  ],
  "extends": ["airbnb"],
  "env": {
    "browser": true,
    "commonjs": true,
    "node": true,
    "es6": true
  },
  "parserOptions": {
    "ecmaVersion": 2018
  },
  "parser": "babel-eslint",
  "rules": {
    "no-console": "off",
    "strict": ["error", "global"],
    "curly": "warn",
    "import/no-unresolved": 0,
    "react/jsx-filename-extension": [1, { "extensions": [".js", ".jsx"] }],
    "global-require": "warn"
  }
}